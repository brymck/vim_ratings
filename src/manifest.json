{
  "name": "vim_ratings",
  "version": "0.8",
  "description": "Show additional rating metrics for vim plugins at vim.org",
  "icons": {
    "16": "img/icon_16.png",
    "48": "img/icon_48.png",
    "128": "img/icon_128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://www.vim.org/*"],
      "js": [
        "js/jquery-1.6.1.min.js",
        "js/xlcolorscale-0.5.min.js",
        "js/content.js"
      ]
    }
  ]
}
